{
	"compilerOptions": {
		"target": "ESNext",
		"lib": [
			"ESNext",
			"esnext"
		],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"resolveJsonModule": true,
		"strict": true,
		"strictNullChecks": true,
		"esModuleInterop": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true
	},
	"include": [
		"app.ts",
		"types/**/*.d.ts"
	],
}