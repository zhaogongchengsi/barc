{
  "name": "@zunh/barc",
  "version": "0.0.0",
  "packageManager": "pnpm@9.4.1",
  "description": "barc",
  "private": false,
  "homepage": "https://github.com/zhaozunhong/onion#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhaozunhong/onion.git"
  },
  "bugs": "https://github.com/zhaozunhong/onion/issues",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && npm publish --access=public",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "bin": {
    "barc": "./bin/index.mjs"
  },
  "author": "zhaozunhong",
  "license": "MIT",
  "keywords": [],
  "devDependencies": {
    "@types/node": "^20.14.10",
    "bumpp": "^9.4.1",
    "esno": "^4.7.0",
    "unbuild": "^2.0.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "destr": "^2.0.3"
  }
}
